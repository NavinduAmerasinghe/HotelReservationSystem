<?xml version="1.0" encoding="UTF-8"?>
<api context="/users" name="HotelLankaUsersAPI" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="DELETE POST PUT PATCH GET">
        <inSequence>
            <log>
                <property name="message" value="Welcome To Hotel Lanka User Service"/>
            </log>
            <send>
                <endpoint key="userServiceEndpoint"/>
            </send>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
        <faultSequence/>
    </resource>
</api>
